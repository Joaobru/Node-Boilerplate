{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["standard-with-typescript", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
		"prettier"
	],
    "rules": {
        "prettier/prettier": [
			"error",
			{
				"endOfLine": "auto"
			}
		]
    }
}
